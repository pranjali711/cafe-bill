<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.karan.cafebill">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CafeBill">
        <activity android:name="com.karan.cafebill.OrderComplete"></activity>
        <activity android:name="com.karan.cafebill.view.RevenueviewActivity" />
        <activity android:name="com.karan.cafebill.view.OrderviewActivity" />
        <activity android:name="com.karan.cafebill.view.HistoryviewActivity" />
        <activity android:name="com.karan.cafebill.view.CartviewActivity" />
        <activity android:name="com.karan.cafebill.DashboardActivity" />
        <activity android:name="com.karan.cafebill.Splashscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.karan.cafebill.LoginActivity" />
    </application>

</manifest>